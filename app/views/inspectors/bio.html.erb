<%= form_for(@inspector, url:"/inspectors/#{@inspector.id}/bio", method:"post") do |f| %>
    <div class="field">
      <%= f.label :bio %><br />
      <%= f.text_area :bio, value: current_inspector.bio, cols: 80, rows: 5 %>
    </div>
    <div class="actions">
      <%= f.submit "Submit your Bio" %>
    </div>
<% end %>
